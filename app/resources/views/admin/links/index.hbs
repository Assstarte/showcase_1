<div class="container mb-5">
    <h1>Links</h1>
    <div class="row">
        <div class="col">
            <h3>Raw Input</h3>
            <form action="">
                <textarea class="form-control" name="rawLinks" id="" cols="30" rows="10"></textarea>
                <button type="submit" class="btn btn-default btn-success mt-3">Submit RAW</button>
            </form>

            <h3 class="mt-5">FileInput</h3>
            <form action="">
                <input type="file" name="fileLinks" class="form-control">
                <button type="submit" class="btn btn-default btn-success mt-3">Submit File</button>
            </form>
        </div>
    </div>
</div>